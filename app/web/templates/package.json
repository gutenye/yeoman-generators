{
  "name": "web",
  "version": "0.0.1",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --env=jsdom",
    "lint": "npm run lint:js & npm run lint:css",
    "lint:js": "eslint src/**/*.js",
    "lint:css": "scss-lint",
    "build:docs": "styleguidist build"
  },
  "apollo": {
    "schemas": {
      "default": {
        "endpoint": "http://localhost:4011/graphql"
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "eslintConfig": {
    "extends": "guten-typescript-react"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:4011",
      "pathRewrite": {
        "^/api": ""
      }
    },
    "/uploads": {
      "target": "http://localhost:4011"
    }
  },
  "dependencies": {
    "@gutenye/react-icon-base": "^0.0.0",
    "api-error-handler": "^0.0.0",
    "apollo-client-preset": "^0.0.0",
    "apollo-link-context": "^0.0.0",
    "apollo-link-error": "^0.0.0",
    "apollo-upload-client": "^0.0.0",
    "frisbee": "^0.0.0",
    "classnames": "^0.0.0",
    "graphql-tag": "^0.0.0",
    "gureact": "^0.0.0",
    "lodash": "^0.0.0",
    "mobx": "^0.0.0",
    "mobx-react": "^0.0.0",
    "mobx-react-devtools": "^0.0.0",
    "prop-types": "^0.0.0",
    "react": "^0.0.0",
    "react-apollo": "^0.0.0",
    "react-dom": "^0.0.0",
    "react-helmet": "^0.0.0",
    "react-icons": "^0.0.0",
    "react-router-dom": "^0.0.0",
    "react-tap-event-plugin": "^0.0.0",
    "recompose": "^0.0.0",
    "styled-components": "next",
    "react-ga": "^0.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "react-scripts": "^0.0.0",
    "@craco/craco "^0.0.0",
    "source-map-explorer": "^0.0.0",
    "react-docgen-typescript": "^1.8.1",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "description": "hello",
  "license": "MIT"
}
