{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production NODE_PATH=\"lib:$NODE_PATH\" node lib/server.js",
    "build": "tsc",
    "build:docs": "./ake build:docs",
    "test": "jest",
    "setup": "NODE_ENV=production NODE_PATH=\"lib:$NODE_PATH\" babel-node scripts/setup.js",
    "migrate": "NODE_ENV=production NODE_PATH=\"lib:$NODE_PATH\" babel-node scripts/migrate.js"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTest.ts",
    "testMatch": [
      "**/__tests__/**/*.test.(ts|js)"
    ]
  },
  "eslintConfig": {
    "extends": "guten-typescript"
  },
  "dependencies": {
    "date-fns": "^2.0.0-alpha.7",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "graphql-subscriptions": "^0.5.8",
    "graphql-type-json": "^0.2.1",
    "guserver": "^1.0.39",
    "guserver-auth-magic": "^1.0.17",
    "guserver-schema-mini": "^1.0.13",
    "pdjs": "^1.0.6",
    "pg": "^7.4.1",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.0-alpha.19",
    "bcrypt": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-guten-typescript": "^2.0.1",
    "gudatagen": "^3.0.1",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "ts-jest": "^22.0.4",
    "ts-node": "^6.0.1",
    "typescript": "^2.7.1"
  },
  "author": "",
  "license": "ISC"
}
